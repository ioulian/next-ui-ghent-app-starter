{
  "name": "next-io-ghent-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky && panda codegen",
    "postinstall": "husky && panda codegen",
    "dev": " rm -rf .next && cross-env NODE_ENV=development panda codegen && next dev",
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "yarn check-build-prerequisites && rm -rf .next && panda codegen && next build",
    "start": "next start",
    "check-types": "tsc --noemit",
    "format-code": "prettier --write .",
    "lint": "yarn check-types && next lint --fix",
    "check-translations": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-translations.ts",
    "check-build-prerequisites": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-build-prerequisites.ts",
    "storybook": "cross-env NODE_ENV=development panda codegen && storybook dev -p 6006",
    "build-storybook": "panda codegen && storybook build"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.9",
    "@t3-oss/env-core": "^0.9.2",
    "@tabler/icons": "^2.47.0",
    "colorjs.io": "^0.4.5",
    "easing-coordinates": "^2.0.2",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "next-intl": "^3.7.0",
    "postcss": "^8.4.35",
    "react": "^18",
    "react-animate-height": "^3.2.3",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "react-paginate": "^8.2.0",
    "react-select": "^5.8.0",
    "react-tabs": "^6.0.2",
    "react-toastify": "^10.0.4",
    "react-use": "^17.5.0",
    "server-only": "^0.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@csstools/postcss-cascade-layers": "^4.0.3",
    "@next/bundle-analyzer": "^14.1.0",
    "@pandacss/dev": "^0.32.1",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/blocks": "^7.6.16",
    "@storybook/nextjs": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "@stylistic/eslint-plugin": "^1.6.1",
    "@types/lodash": "^4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/svg-sprite-loader": "^3.9.9",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "cssnano": "^6.0.5",
    "eslint": "^8",
    "eslint-config-next": "^14.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^9.0.11",
