{
  "name": "next-io-ghent-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky && panda codegen",
    "postinstall": "husky && panda codegen && patch-package",
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 rm -rf .next && cross-env NODE_ENV=development panda codegen && next dev --experimental-https",
    "analyze": "cross-env ANALYZE=true npm run build",
    "build": "npm run check-translations && npm run check-build-prerequisites && rm -rf .next && panda codegen && next build",
    "start": "next start",
    "check-types": "tsc --noemit",
    "format-code": "prettier --write .",
    "lint": "npm run check-types && next lint --fix",
    "check-translations": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-translations.ts",
    "check-build-prerequisites": "ts-node -O '{\"module\":\"commonjs\"}' scripts/check-build-prerequisites.ts",
    "storybook": "cross-env NODE_ENV=development panda codegen && storybook dev -p 6006",
    "build-storybook": "panda codegen && storybook build",
    "test-storybook": "test-storybook",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.10",
    "@tabler/icons": "^2.47.0",
    "colorjs.io": "^0.5.0",
    "date-fns": "^3.3.1",
    "easing-coordinates": "^2.0.2",
    "lodash": "^4.17.21",
    "next": "14.1.4",
    "next-intl": "^3.9.1",
    "next-nprogress-bar": "^2.3.2",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.35",
    "react": "^18",
    "react-animate-height": "^3.2.3",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.0",
    "react-paginate": "8.2.0",
    "react-select": "^5.8.0",
    "react-tabs": "^6.0.2",
    "react-toastify": "^10.0.5",
    "react-use": "^17.5.0",
    "server-only": "^0.0.1",
    "swiper": "^11.0.7",
    "unstorage": "^1.10.2",
    "webpack": "^5.90.3"
  },
  "devDependencies": {
    "@csstools/postcss-cascade-layers": "^4.0.4",
    "@next/bundle-analyzer": "14.1.3",
    "@pandacss/dev": "^0.33.0",
    "@storybook/addon-a11y": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/components": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@storybook/test": "^8.0.4",
    "@storybook/test-runner": "^0.17.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/svg-sprite-loader": "^3.9.9",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "cssnano": "^6.0.5",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mini-css-extract-plugin": "^2.8.1",
    "next-cache-toolbar": "^0.2.1",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^3.2.5",
    "react-polymorphed": "^2.2.2",
    "storybook": "^8.0.6",
    "style-loader": "^3.3.4",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
